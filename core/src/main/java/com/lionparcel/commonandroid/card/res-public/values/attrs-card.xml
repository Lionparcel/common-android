<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LPCard">
        <attr name="cardStyle">
            <enum name="shadow" value="0"/>
            <enum name="solid" value="1"/>
            <enum name="banner" value="2"/>
            <enum name="border" value="3"/>
            <enum name="list" value="4"/>
        </attr>
        <attr name="cardTitle" format="string"/>
        <attr name="cardSubtitle" format="string"/>
        <attr name="cardsContent" format="string"/>
        <attr name="buttonText" format="string"/>
    </declare-styleable>
</resources>