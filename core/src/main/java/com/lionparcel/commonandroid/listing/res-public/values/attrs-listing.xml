<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LPListing">
        <attr name="listStyle" format="enum">
            <enum name="one_line" value="0"/>
            <enum name="two_line" value="1"/>
            <enum name="with_thumbnail" value="2"/>
        </attr>
        <attr name="addListDivider" format="boolean"/>
        <attr name="iconStart" format="enum">
            <enum name="none" value="0"/>
            <enum name="radio_button" value="1"/>
            <enum name="check_box" value="2"/>
            <enum name="remove_button" value="3"/>
            <enum name="icon" value="4"/>
        </attr>
        <attr name="iconEnd" format="enum">
            <enum name="none" value="0"/>
            <enum name="toggle_button" value="1"/>
            <enum name="remove_button" value="2"/>
            <enum name="icon" value="3"/>
            <enum name="button" value="4"/>
        </attr>
        <attr name="buttonText" format="string"/>
        <attr name="iconStartImage" format="reference"/>
        <attr name="iconEndImage" format="reference"/>
    </declare-styleable>
</resources>