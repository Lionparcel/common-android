<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LPAccordion">
        <attr name="accordionSize" format="enum">
            <enum name="large" value="0"/>
            <enum name="medium" value="1"/>
            <enum name="small" value="2"/>
        </attr>
        <attr name="accordionMargin" format="enum">
            <enum name="spacing_medium" value="0"/>
            <enum name="spacing_large" value="1"/>
            <enum name="spacing_extra_large" value="2"/>
        </attr>
        <attr name="accordionStyle" format="enum">
            <enum name="border" value="0"/>
            <enum name="no_border" value="1"/>
        </attr>
        <attr name="accordionExpandContent" format="enum">
            <enum name="without_text" value="0"/>
            <enum name="with_text" value="1"/>
        </attr>
    </declare-styleable>
</resources>