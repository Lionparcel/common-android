<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/llExampleList"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/size_12"
        android:paddingBottom="@dimen/size_12"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvSampleList"
            style="@style/Subtitle3Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/shades7"
            android:text="TEST"/>

        <ImageView
            android:id="@+id/ivCheckItem"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="7dp"
            app:srcCompat="@drawable/ic_ics_o_check"/>


    </LinearLayout>

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        style="@style/DividerStyle"
        android:layout_above="@+id/flButton" />

</LinearLayout>

