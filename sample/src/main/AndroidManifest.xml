<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lionparcel.commonandroidsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Commonandroid">
        <activity
            android:name=".toastsnackbar.SnackbarSampleActivity"
            android:exported="false" />
        <activity android:name=".toastsnackbar.ToastSampleActivity" />
        <activity
            android:name=".walktrough.WalkThroughSampleActivity"
            android:exported="true" />
        <activity
            android:name=".form.FormComponentSampleActivity"
            android:exported="true" />
        <activity android:name=".loading.LoadingComponentSampleActivity" />
        <activity android:name=".loading.LoadingSpinnerDefaultSampleActivity" />
        <activity android:name=".loading.LoadingShimmerSampleActivity" />
        <activity android:name=".modal.ModalComponentSampleActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
